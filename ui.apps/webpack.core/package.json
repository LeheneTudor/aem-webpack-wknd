{
    "name": "wknd-webpack-example",
    "version": "1.0.0",
    "private": true,
    "description": "AEM Webpack with dynamic imports",
    "scripts": {
        "aem:front": "cross-env aem-front -w ../src/main/content/jcr_root/apps/wknd/clientlibs/webpack.bundles/resources -i '700' ",
        "aem:sync": "cross-env node internals/aemsync.js",
        "build": "cross-env webpack --config internals/webpack.config.js",
        "build:dev": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 npm run build",
        "build:debug": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 npm run build --log-level=debug",
        "build:devenv": "cross-env NODE_ENV=devenv BABEL_DISABLE_CACHE=1 npm run build",
        "build:watch": "cross-env NODE_ENV=development npm run build -- --watch",
        "production": "cross-env NODE_ENV=production npm run build -- -p",
        "start": "cross-env FORCE_COLOR=1 CI=true concurrently --prefix \"[ {name} ]\"  --names \"AEM Front, Webpack, AEM Sync \" --prefix-colors \"white.bgCyan.bold,white.bgGreen.bold\" \"nodemon --exec \\\"npm run aem:front\\\"\" \"nodemon --exec \\\"npm run build:watch\\\"\" --kill-others \"nodemon --exec \\\"npm run aem:sync\\\"\" \"nodemon --exec \\\"npm run bind:stop\\\"\"",
        "test:debug": "cross-env BABEL_DISABLE_CACHE=1 npm run test -- --no-cache",
        "test:watch": "cross-env npm run test -- --watch",
        "bind:stop": "node internals/kill-node.js",
        "resolve": "npx npm-force-resolutions",
        "lint": "prettier --check ../../",
        "prettier:fix": "prettier --write ../../"
    },
    "author": "Tudor Lehene",
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "aem-clientlib-generator-webpack-plugin": "^1.0.0",
        "aem-front": "^0.3.0",
        "aemsync": "^4.0.3",
        "autoprefixer": "9.8.0",
        "babel-eslint": "10.1.0",
        "babel-loader": "^8.1.0",
        "browserslist": "^4.16.6",
        "clean-webpack-plugin": "^3.0.0",
        "concurrently": "5.2.0",
        "cross-env": "^6.0.3",
        "css-loader": "^3.5.1",
        "eslint": "^5.10.0",
        "eslint-loader": "^2.2.1",
        "file-loader": "^6.2.0",
        "merge": "^2.1.1",
        "mini-css-extract-plugin": "^0.8.2",
        "node-cleanup": "^2.1.2",
        "nodemon": "^2.0.4",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "postcss-reporter": "^6.0.1",
        "prettier": "2.6.1",
        "pretty-quick": "^3.1.3",
        "sass": "^1.45.1",
        "sass-loader": "^7.3.1",
        "stylelint": "^13.12.0",
        "webpack": "^4.41.5",
        "webpack-command": "^0.5.1",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.6",
        "core-js": "^3.6.5",
        "in-view": "^0.6.1",
        "lightgallery": "^2.6.1",
        "regenerator-runtime": "^0.13.5",
        "swiper": "^8.4.2"
    },
    "resolutions": {
        "trim-newlines": "3.0.1",
        "yargs-parser": "20.2.9",
        "tar": "6.1.11",
        "glob-parent": "^5.1.2"
    },
    "browserslist": [
        "last 2 version",
        "> 1%"
    ]
}
